<odoo>
    <data>
          <template id="splitwise.expense_view">
            <t t-call="web.frontend_layout">
                <t t-set="head">
                    <t t-call-assets="splitwise.assets_splitwise" lazy_load="True"/>
                </t>
                <t t-set="no_header" t-value="1"/>
                <t t-set="no_footer" t-value="1"/>
                <t t-set="no_livechat" t-value="1"/>
                <owl-component name="splitwise" />
            </t>
          </template>
    </data>
</odoo>
